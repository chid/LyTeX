% simplified Chinese characters
%
% character set: GB 2312-80
% font encoding: pmC

\DeclareFontFamily{C11}{kai}{\hyphenchar \font\m@ne}

\DeclareFontShape{C11}{kai}{m}{n}{<-> CJK * gbkai}{}
\DeclareFontShape{C11}{kai}{bx}{n}{<-> CJKb * gbkai}{\CJKbold}
\DeclareFontShape{C11}{kai}{m}{it}{<-> CJK * gbkaisl}{}
\DeclareFontShape{C11}{kai}{bx}{it}{<-> CJK * gbkaisl}{\CJKbold}
\DeclareFontShape{C11}{kai}{m}{sl}{<-> CJK * gbkaisl}{}
\DeclareFontShape{C11}{kai}{bx}{sl}{<-> CJK * gbkaisl}{\CJKbold}

\endinput
