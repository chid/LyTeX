% simplified Chinese characters
%
% character set: GB 2312-80
% font encoding: pmC

\DeclareFontFamily{C11}{li}{\hyphenchar \font\m@ne}

\DeclareFontShape{C11}{li}{m}{n}{<-> CJK * gbli}{}
\DeclareFontShape{C11}{li}{bx}{n}{<-> CJKb * gbli}{\CJKbold}
\DeclareFontShape{C11}{li}{m}{it}{<-> CJK * gblisl}{}
\DeclareFontShape{C11}{li}{bx}{it}{<-> CJK * gblisl}{\CJKbold}
\DeclareFontShape{C11}{li}{m}{sl}{<-> CJK * gblisl}{}
\DeclareFontShape{C11}{li}{bx}{sl}{<-> CJK * gblisl}{\CJKbold}

\endinput
